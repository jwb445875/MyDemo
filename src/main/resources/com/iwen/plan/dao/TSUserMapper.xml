<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iwen.plan.dao.UserMapper">

	<select id="getUserByAccount" resultType="com.iwen.plan.bean.TSUser">
		SELECT id,username as account from t_s_base_user  where username=#{account} LIMIT 0,1
	</select>


</mapper>
